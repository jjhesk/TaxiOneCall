<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:top="-1dp"
        android:right="-1dp"
        android:left="-2dp"
        android:bottom="2dp">
        <shape>
            <stroke
                android:width="10dp"
                android:color="@color/divider_press" />
        </shape>
    </item>

    <item
        android:top="-1dp"
        android:bottom="1dp">
        <shape android:shape="rectangle">
            <solid android:color="@android:color/white" />
        </shape>
    </item>


</layer-list>